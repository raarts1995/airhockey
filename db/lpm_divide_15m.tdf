FUNCTION sign_div_unsign_5nh (denominator[11..0], numerator[11..0])
RETURNS ( quotient[11..0], remainder[11..0]);

--synthesis_resources = lut 90 
SUBDESIGN lpm_divide_15m
( 
	denom[11..0]	:	input;
	numer[10..0]	:	input;
	quotient[10..0]	:	output;
	remain[11..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_5nh;
	gnd_wire	: WIRE;
	num_padder[0..0]	: WIRE;
	numer_tmp[11..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	gnd_wire = B"0";
	num_padder[] = ( gnd_wire);
	numer_tmp[] = ( num_padder[], numer[]);
	quotient[10..0] = divider.quotient[10..0];
	remain[] = divider.remainder[];
END;
